{
  "cli": {
    "version": "9.0.3",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "env": {
    "EXPO_PUBLIC_BASE_API_URL": "https://api.example.com",
    "EXPO_PUBLIC_WEB_APP_API_URL_TEMPLATE": "https://{ACCOUNT_NAME}.example.com",
    "EXPO_PUBLIC_VIDEO_UPLOAD_URL": "https://upload.example.com/videos"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ACCOUNT_NAME_PLACEHOLDER": "ACCOUNT_NAME",
        "EXPO_PUBLIC_BASE_API_URL": "https://api.example.com",
        "EXPO_PUBLIC_WEB_APP_API_URL": "https://dev.example.com",
        "EXPO_PUBLIC_VIDEO_UPLOAD_URL": "https://upload.example.com/videos"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": { "simulator": true },
      "android": { "emulator": true },
      "env": {
        "EXPO_PUBLIC_ACCOUNT_NAME_PLACEHOLDER": "ACCOUNT_NAME",
        "EXPO_PUBLIC_BASE_API_URL": "https://api.example.com",
        "EXPO_PUBLIC_WEB_APP_API_URL": "https://preview.example.com",
        "EXPO_PUBLIC_VIDEO_UPLOAD_URL": "https://upload.example.com/videos"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_ACCOUNT_NAME_PLACEHOLDER": "ACCOUNT_NAME",
        "EXPO_PUBLIC_BASE_API_URL": "https://api.example.com",
        "EXPO_PUBLIC_WEB_APP_API_URL": "https://prod.example.com",
        "EXPO_PUBLIC_VIDEO_UPLOAD_URL": "https://upload.example.com/videos"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "YOUR_IOS_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-your-google-service-account.json"
      }
    }
  },
  "cache": {
    "disabled": false,
    "key": ["yarn.lock", "package.json"]
  },
  "hooks": {
    "pre-build": "scripts/prebuild.sh",
    "post-build": "scripts/postbuild.sh"
  }
}
